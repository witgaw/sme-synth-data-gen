{
  "meta": {
    "version": "1.0",
    "created": "2026-01-15",
    "description": "CRM/Project management database for Kreatywna Fala",
    "database_name": "kreatywna_fala_crm.db",
    "notes": "SQLite database with realistic CRM data. Some facts are DB-only, others require cross-referencing with documents."
  },
  "schema": {
    "clients": {
      "description": "Client companies",
      "columns": {
        "id": "INTEGER PRIMARY KEY",
        "code": "TEXT UNIQUE NOT NULL",
        "name": "TEXT NOT NULL",
        "nip": "TEXT",
        "address": "TEXT",
        "city": "TEXT",
        "industry": "TEXT",
        "status": "TEXT",
        "acquired_date": "DATE",
        "account_manager_id": "INTEGER REFERENCES employees(id)",
        "notes": "TEXT"
      }
    },
    "contacts": {
      "description": "Contact persons at client companies",
      "columns": {
        "id": "INTEGER PRIMARY KEY",
        "client_id": "INTEGER REFERENCES clients(id)",
        "name": "TEXT NOT NULL",
        "email": "TEXT",
        "phone": "TEXT",
        "role": "TEXT",
        "is_primary": "BOOLEAN DEFAULT 0",
        "notes": "TEXT"
      }
    },
    "employees": {
      "description": "Agency staff",
      "columns": {
        "id": "INTEGER PRIMARY KEY",
        "name": "TEXT NOT NULL",
        "email": "TEXT UNIQUE",
        "role": "TEXT",
        "department": "TEXT",
        "hire_date": "DATE",
        "hourly_rate": "DECIMAL(10,2)",
        "is_active": "BOOLEAN DEFAULT 1"
      }
    },
    "projects": {
      "description": "Client projects and campaigns",
      "columns": {
        "id": "INTEGER PRIMARY KEY",
        "client_id": "INTEGER REFERENCES clients(id)",
        "name": "TEXT NOT NULL",
        "type": "TEXT",
        "status": "TEXT",
        "budget_net": "DECIMAL(10,2)",
        "start_date": "DATE",
        "end_date": "DATE",
        "lead_employee_id": "INTEGER REFERENCES employees(id)",
        "notes": "TEXT"
      }
    },
    "project_assignments": {
      "description": "Employee assignments to projects",
      "columns": {
        "id": "INTEGER PRIMARY KEY",
        "project_id": "INTEGER REFERENCES projects(id)",
        "employee_id": "INTEGER REFERENCES employees(id)",
        "role": "TEXT",
        "assigned_date": "DATE"
      }
    },
    "time_entries": {
      "description": "Hours logged by employees on projects",
      "columns": {
        "id": "INTEGER PRIMARY KEY",
        "project_id": "INTEGER REFERENCES projects(id)",
        "employee_id": "INTEGER REFERENCES employees(id)",
        "date": "DATE NOT NULL",
        "hours": "DECIMAL(4,2) NOT NULL",
        "description": "TEXT",
        "billable": "BOOLEAN DEFAULT 1"
      }
    },
    "invoices": {
      "description": "Invoices issued to clients",
      "columns": {
        "id": "INTEGER PRIMARY KEY",
        "client_id": "INTEGER REFERENCES clients(id)",
        "project_id": "INTEGER REFERENCES projects(id)",
        "invoice_number": "TEXT UNIQUE NOT NULL",
        "amount_net": "DECIMAL(10,2) NOT NULL",
        "vat_rate": "DECIMAL(4,2) DEFAULT 23.00",
        "amount_gross": "DECIMAL(10,2)",
        "issued_date": "DATE NOT NULL",
        "due_date": "DATE",
        "paid_date": "DATE",
        "status": "TEXT DEFAULT 'issued'"
      }
    },
    "expenses": {
      "description": "Vendor expenses and subcontractor costs",
      "columns": {
        "id": "INTEGER PRIMARY KEY",
        "project_id": "INTEGER REFERENCES projects(id)",
        "vendor_name": "TEXT NOT NULL",
        "vendor_nip": "TEXT",
        "invoice_number": "TEXT",
        "description": "TEXT",
        "amount_net": "DECIMAL(10,2) NOT NULL",
        "amount_gross": "DECIMAL(10,2)",
        "expense_date": "DATE",
        "category": "TEXT"
      }
    }
  },
